<?xml version="1.0" encoding="utf-8" ?>
<Application
    x:Class="Dorexcel.App"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:converter="using:Dorexcel.Converter"
    xmlns:local="using:Dorexcel">
    <Application.Resources>
        <ResourceDictionary>
            <ResourceDictionary.MergedDictionaries>
                <XamlControlsResources xmlns="using:Microsoft.UI.Xaml.Controls" />
                <!--  Other merged dictionaries here  -->
            </ResourceDictionary.MergedDictionaries>
            <!--  Other app resources here  -->
            <converter:BoolToVisibilityConverter x:Key="BoolToVisibility" />
            <converter:NotBoolToVisibilityConverter x:Key="NotBoolToVisibility" />
            <converter:BoolToOpacityConverter x:Key="BoolToOpacity" />
            <converter:NotBoolToOpacityConverter x:Key="NotBoolToOpacity" />
            <converter:EnterColumnConverter x:Key="EnterColumn" />
            <converter:IdEmphasisConverter x:Key="IdEmphasis" />
        </ResourceDictionary>
    </Application.Resources>
</Application>
